    <!--Script for creating Menu -->
    <script language="javascript" type="text/javascript">
	    
	<!-- funci[on para renderizar los datos de la vista -->
        $(document).ready(function () {
            CreateMenu();
        });

	<!-- funcion llamado ajax para poder extraer el menu de un archivo xml -->
        function CreateMenu() {
            $.ajax({
                url: "/Home/MenuList",
                type: "GET",
                dataType: "json",
                success: function (data) {
                    console.log(data.menuList);
                    createMenu(data.menuList);
                },
                error: function (req, status, error) {
                    console.log(status);
                }
            });
        }

	<!-- funcion que crea el sidebar dependiendo de la longitud y los datos extraidos del xml -->
        function createMenu(menu) {

            $(".slideDashboard").html("<a class='side-menu__item' data-bs-toggle='slide' href=" + menu[0].url + ">" + "<i class='side-menu__icon " + menu[0].icon + "'></i> <span class='side-menu__label'>" + menu[0].nameMenu + "</span></a>");

            $(".slideReport").html("<a class='side-menu__item' data-bs-toggle='slide' href=" + menu[1].url + ">" + "<i class='side-menu__icon " + menu[1].icon + "'></i> <span class='side-menu__label'>" + menu[1].nameMenu + "</span></a>");

        }

       

    </script>

